
@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>KayitOl</title>
    <link rel="stylesheet" href="~/app/assets/vendor/css/core.css" class="template-customizer-core-css" />
    <link rel="stylesheet" href="~/app/assets/vendor/css/StyleSheet1.css" class="template-customizer-theme-css" />
    <link rel="stylesheet" href="~/app/assets/css/demo.css" />

    <link rel="stylesheet" href="~/app/assets/vendor/libs/perfect-scrollbar/perfect-scrollbar.css" />

</head>
<body>
    <div class="container mt-5">
        <div><h3 style="margin:20px;text-align:center" class="text-info">KAYIT OL</h3></div>
        @using (Html.BeginForm("KayitOl", "Login", FormMethod.Post))
        {
            <div class="form-group">
                <h5 style="margin:10px" class="text-info">Ad</h5>
                <input type="text" name="Ad" class="form-control" />
            </div>
            <div class="form-group">
                <h5 style="margin:10px" class="text-info">Soyad</h5>
                <input type="text" name="Soyad" class="form-control" />
            </div>
            <div class="form-group">
                <h5 style="margin:10px" class="text-info">Şifre</h5>
                <input type="text" name="Sifre" class="form-control" />
            </div>
            <div class="form-group">
                <h5 style="margin:10px" class="text-info">Email</h5>
                <input type="text" name="Email" class="form-control" />
            </div>
            <br />
            <button class="btn btn-info">Gönder</button>
        }
    </div>
</body>
</html>
