@model Email
@{
    ViewBag.Title = "GuncelleGet";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}

<h2>Güncelle</h2>
@using (Html.BeginForm("GuncellePost", "Email", FormMethod.Post))
{
    <div class="form-group">
        @Html.Label("Email")
        <input class="form-control" type="text" name="email[0].Email1" value="@Model.Email1" />

    </div>
    <div class="form-group" style="display:none">
        <input class="form-control" type="text" name="email[1].Email1" value="@Model.Email1" />
        <input class="form-control" type="text" name="email[1].KullaniciID" value="@Model.KullaniciID" />

    </div>

    <br />
    
    <button type="submit" class="btn btn-warning">Güncelle</button>



}


